<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.review.manage.variate.mapper.VariateGradeMapper">
    <delete id="deleteVariateGrade">
        delete from review_variate_grade
        where variate_id IN
        <foreach collection="variateIds" index="index" item="variateId" open="(" separator="," close=")">
            #{variateId1}
        </foreach>
    </delete>
</mapper>