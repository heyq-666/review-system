<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.review.manage.intake.mapper.ReviewIntakeMapper">
    <select id="getDepartNameList" resultType="String">
        select depart_name as companyName from sys_depart where id in
        <foreach collection="companyId" index="index" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </select>
</mapper>